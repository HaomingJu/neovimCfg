{
  "npm.binPath": "/usr/local/bin/cnpm",
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "notification.winblend": 30,
  "suggest.snippetIndicator": "*",
  "suggest.noselect": false,
  "suggest.completionItemKindLabels": {
    "method": "Method",
    "function": "Function",
    "constructor": "Constructor",
    "field": "Field",
    "variable": "Variable",
    "class": "Class",
    "interface": "Interface",
    "module": "Module",
    "property": "Property",
    "unit": "Unit",
    "value": "Value",
    "enum": "Enum",
    "keyword": "Keyword",
    "snippet": "Snippet",
    "text": "Text",
    "color": "Color",
    "file": "File",
    "reference": "Ref",
    "folder": "Floder",
    "enumMember": "Enum",
    "constant": "Const",
    "struct": "Struct",
    "event": "Event",
    "operator": "Operator",
    "typeParameter": "TypeParam",
    "default": "Default"
  },
  "suggest.formatItems": [
    "abbr",
    "menu",
    "kind",
    "shortcut"
  ],
  "diagnostic.errorSign": "E>",
  "diagnostic.warningSign": "W>",
  "diagnostic.infoSign": "I>",
  "suggest.removeDuplicateItems": true,
  "snippets.priority": 100,
  "snippets.snipmate.enable": false,
  "snippets.ultisnips.enable": true,
  "snippets.ultisnips.directories": [
    "~/.config/Snippets"
  ],
  "snippets.userSnippetsDirectory": "~/.config/Snippets",
  "notification.border": false,
  "python.pythonPath": "/usr/bin/python2.7",
  "python.linting.pylintEnabled": true,
  "python.linting.enabled": false,
  "clangd.arguments": [
    "--completion-style=detailed",
    "--pch-storage=memory",
    "--clang-tidy",
    "--enable-config",
    "--query-driver=/opt/clangd_*/lib/clang/*/include",
    "--compile-commands-dir=build-x86"
  ],
  "clangd.fallbackFlags": [
    "-std=c++17"
  ],
  "inlayHint.enable": false,
  "sumneko-lua.enableNvimLuaDev": true,
  "sumneko-lua.checkUpdate": true,
  "sumneko-lua.prompt": true,
  "Lua.telemetry.enable": true,
  "languageserver": {
    "digestif": {
      "command": "digestif",
      "filetypes": [
        "tex",
        "plaintex",
        "context"
      ]
    }
  }
}
